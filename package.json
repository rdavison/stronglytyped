    "@opam/js_of_ocaml": {
      "source": "git+ssh://git@github.com:ocsigen/js_of_ocaml:js_of_ocaml.opam#efef21c6e6837a87d439ddb04e30f446b7772a03",
      "override": {
        "dependencies": {
          "@opam/js_of_ocaml-compiler": "*"
        }
      }
    },
    "@opam/js_of_ocaml-compiler": {
      "source": "git+ssh://git@github.com:ocsigen/js_of_ocaml:js_of_ocaml-compiler.opam#efef21c6e6837a87d439ddb04e30f446b7772a03",
