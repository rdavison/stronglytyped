{
  "name": "stronglytyped",
  "version": "0.0.1",
  "description": "Keyboard layout analyzer and generator",
  "esy": {
    "build": "dune build --root #{self.root} --profile=prod @install",
    "buildDev": "dune build --root #{self.root} --profile=dev @all",
    "buildsInSource": "_build"
  },
  "dependencies": {
    "@opam/async": "*",
    "@opam/async_js": "*",
    "@opam/async_unix": "*",
    "@opam/core": "v0.15.0",
    "@opam/core_unix": "*",
    "@opam/dune": "*",
    "@opam/dune-site": "*",
    "@opam/incremental": "*",
    "@opam/incr_map": "*",
    "@opam/incr_dom": "*",
    "@opam/notty": "*",
    "@opam/notty_async": "*",
    "@opam/ppx_jane": "*",
    "@opam/sexp_pretty": "*",
    "@opam/textutils_kernel": "*",
    "ocaml": "4.14.0"
  },
  "devDependencies": {
    "@opam/ocaml-lsp-server": "*",
    "@opam/ocamlformat": "*",
    "@opam/ocamlformat-rpc": "*",
    "@opam/js_of_ocaml-compiler": "*",
    "@opam/utop": "*"
  },
  "resolutions": {
